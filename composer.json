{
    "name": "brunocfalcao/binance-api",
    "description": "Binance API tweaked for futures",
    "keywords": [
        "brunocfalcao"
    ],
    "homepage": "https://github.com/brunocfalcao/binance-api",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ratchet/pawl": "^0.3.0",
        "react/socket": "^1.0 || ^0.8 || ^0.7",
        "ratchet/rfc6455": "^0.3",
        "laravel/ui": "^3.3",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "BinanceApi\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BinanceApi\\BinanceApiServiceProvider"
            ]
        }
    }
}
